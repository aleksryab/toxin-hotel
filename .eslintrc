{
	"extends": "eslint:recommended",
	"env": {
	  "browser": true,
	  "node": true,
	  "es6": true,
	  "jquery": true
	},
	"rules": {
    "semi": ["error", "always"],
    "no-console": 0,
    "no-unused-vars": ["warn", { "vars": "all", "args": "none", "ignoreRestSiblings": false }]
	},
	"parserOptions": {
		"sourceType": "module",
    "allowImportExportEverywhere": true
    }
  }